1. Selection Sort

function selectionSort(arr){
    for (let i = 0; i < arr.length - 1; i++) {
    let mini = i;
    for (let j = i + 1; j < arr.length; j++) {
      if (arr[j] < arr[mini]) {
        mini = j;
      }
    }
    let temp = arr[mini];
    arr[mini] = arr[i];
    arr[i] = temp;
  }
  console.log(arr,"arr")
}

selectionSort([4,1,3,9,7])
